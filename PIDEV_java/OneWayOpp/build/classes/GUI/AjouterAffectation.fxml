<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.AjouterAffectationController">
   <children>
      <ComboBox fx:id="comboOp" layoutX="126.0" layoutY="129.0" prefWidth="150.0" promptText="idOp" />
      <Button fx:id="ajouterbtn" layoutX="360.0" layoutY="70.0" mnemonicParsing="false" onAction="#AjouterAffectation" text="Ajouter" />
      <Button fx:id="afficherbtn" layoutX="360.0" layoutY="116.0" mnemonicParsing="false" onAction="#AfficherAffectation" text="Afficher" />
      <Label layoutX="35.0" layoutY="25.0" prefHeight="18.0" prefWidth="234.0" text="Affecter colis a une opportunite : ">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboColis" layoutX="126.0" layoutY="83.0" prefWidth="150.0" promptText="idColis" />
      <Button fx:id="modifierbtnn" layoutX="362.0" layoutY="171.0" mnemonicParsing="false" onAction="#modifierAffectation" text="Modifier" />
   </children>
</AnchorPane>
